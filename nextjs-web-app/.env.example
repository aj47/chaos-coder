# Hugging Face API token
# Get this from https://huggingface.co/settings/tokens
HF_API_TOKEN=your_huggingface_api_token

# Supabase configuration
# Get these from your Supabase project settings
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Optional: Portkey API key if using Portkey
# Get this from https://portkey.ai
PORTKEY_API_KEY=your_portkey_api_key

# Google OAuth credentials
# Get these from Google Cloud Console
# These are not needed in the .env file as they are configured in Supabase
# GOOGLE_CLIENT_ID=your_google_client_id
# GOOGLE_CLIENT_SECRET=your_google_client_secret

# Stripe Configuration
# Get these from your Stripe Dashboard
STRIPE_SECRET_KEY=your_stripe_secret_key
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret
STRIPE_PRO_PLAN_PRICE_ID=your_stripe_pro_plan_price_id
STRIPE_ULTRA_PLAN_PRICE_ID=your_stripe_ultra_plan_price_id 